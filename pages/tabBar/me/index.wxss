/* pages/tabBar/me/index.wxss */
.blank{
  width: 100vw;
  height: 10vw;
}
.userblank{
  position: relative;
  background-color: rgb(250,250,250);
  background: linear-gradient(rgb(147,90,245),white);
  width: 100vw;
  height: 50vw;
}
.top_screen{
  width: 100vw;
  height: 10vw;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 5vw;
  font-weight: bold;
}
.userblank_left{
  width: 40vw;
  height: 30vw;
  position: absolute;
}
.userblank_left_image{
  position: absolute;
  top: 1vw;
  left: 11vw;
  width: 20vw;
  height: 20vw;
}
.userblank_right{
  position: absolute;
  width: 60vw;
  height: 30vw;
  top: 23vw;
  left: 38vw;
}
.userblank_right_left_nickname{
  width: 50vw;
  position: absolute;
  font-weight: bold;
}
.userblank_right_left_identity{
  position: absolute;
  font-weight: bold;
  width: 50vw;
  top: 10vw;
}
.userblank_right_right_img{
  position: absolute;
  width: 10vw;
  height: 10vw;
  top: 3vw;
  left: 44vw;
}
.feedback{
  width: 100vw;
  height: 20vw;
  display: flex;
  margin: -2px 0 0 -2px;
  max-height: 150px;
}
.feedback_img{
  position: relative;
  width: 20vw;
  height: 20vw;
  left: 4vw;
}
.feedback_text{
  width: 80vw;
  height: 20vw;
  text-indent: 29vw;
  font-size:9vw;
  font-weight: 540;
  justify-content: center;
  align-items: center;
  display: flex;
}
.version{
  width: 100vw;
  height: 20vw;
  display: flex;
  margin: -2px 0 0 -2px;
  max-height: 150px;
}
.version_img{
  position: relative;
  width: 20vw;
  height: 20vw;
  left: 4vw;
}
.version_text{
  width: 80vw;
  height: 20vw;
  text-indent: 29vw;
  font-size:9vw;
  font-weight: 540;
  justify-content: center;
  align-items: center;
  display: flex;
}